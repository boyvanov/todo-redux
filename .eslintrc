{
  "extends": ["airbnb", "prettier"],
  "plugins": ["import", "jsx-a11y", "react", "react-hooks", "prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "import/prefer-default-export": 0,
    "consistent-return": 0,
    "react/jsx-filename-extension": 0
  },
  "parser": "babel-eslint"
}
